\begin_inset Newline newline
\end_inset


\begin_inset Formula 
\[
x,t=j\cdot dx,n\cdot dt
\]

\end_inset


\begin_inset Formula 
\[
f\left(j\cdot dx,n\cdot dt\right)=f_{j}^{n}
\]

\end_inset


\end_layout

\begin_layout Enumerate
Forward Difference
\begin_inset Newline newline
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula 
\[
\Delta_{dx}\left[f\right]\left(x\right)=f_{j+1}^{n}-f_{j}^{n}
\]

\end_inset


\begin_inset Formula 
\[
\frac{\partial f}{\partial x}\approx\frac{\Delta_{dx}\left[f\right]\left(x\right)}{dx}=\frac{f_{j+1}^{n}-f_{j}^{n}}{dx}
\]

\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Quotes eld
\end_inset

Left
\begin_inset Quotes erd
\end_inset

 Neumann boundary:
\begin_inset Formula 
\[
f^{'}\left(t=0\right)=c\rightarrow\frac{\Delta_{dt}\left[f\right]\left(t=0\right)}{dt}=c
\]

\end_inset


\begin_inset Formula 
\[
\frac{f_{j}^{1}-f_{j}^{0}}{dt}=c\rightarrow f_{j}^{1}=cdt+f_{j}^{0}
\]

\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula 
\[
u^{''}+2\beta u^{'}+\omega_{0}^{2}u=0\rightarrow\frac{\Delta_{dt}^{2}\left[u\right]\left(t\right)}{\left(dt\right)^{2}}+2\beta\frac{\Delta_{dt}\left[u\right]\left(t\right)}{dt}+\omega_{0}^{2}u_{j}^{n}=0
\]

\end_inset


\begin_inset Formula 
\[
\frac{u_{j+2}^{n}-2u_{j+1}^{n}+u_{j}^{n}}{\left(dt\right)^{2}}+2\beta\frac{u_{j+1}^{n}-u_{j}^{n}}{dt}+\omega_{0}^{2}u_{j}^{n}=0
\]

\end_inset


\begin_inset Formula 
\[
u_{j+2}^{n}-2u_{j+1}^{n}+u_{j}^{n}+2\beta\left(u_{j+1}^{n}-u_{j}^{n}\right)dt+\omega_{0}^{2}u_{j}^{n}\left(dt\right)^{2}=0
\]

\end_inset


\begin_inset Formula 
\[
u_{j+2}^{n}=2\left(1-\beta dt\right)u_{j+1}^{n}-\left(1-2\beta dt+\left(\omega_{0}dt\right)^{2}\right)u_{j}^{n}
\]

\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula 
\[
u^{''}+2\beta u^{'}+\omega_{0}^{2}u=f\left(t,u,u^{'}\right)\rightarrow\frac{\Delta_{dt}^{2}\left[u\right]\left(t\right)}{\left(dt\right)^{2}}+2\beta\frac{\Delta_{dt}\left[u\right]\left(t\right)}{dt}+\omega_{0}^{2}u_{j}^{n}=f\left(t,u_{j}^{n},\frac{\Delta_{dt}\left[u\right]\left(t\right)}{dt}\right)
\]

\end_inset


\begin_inset Formula 
\[
\frac{u_{j+2}^{n}-2u_{j+1}^{n}+u_{j}^{n}}{\left(dt\right)^{2}}+2\beta\frac{u_{j+1}^{n}-u_{j}^{n}}{dt}+\omega_{0}^{2}u_{j}^{n}=f\left(t,u_{j}^{n},\frac{u_{j+1}^{n}-u_{j}^{n}}{dt}\right)
\]

\end_inset


\begin_inset Formula 
\[
u_{j+2}^{n}=\left(dt\right)^{2}f\left(t,u_{j}^{n},\frac{u_{j+1}^{n}-u_{j}^{n}}{dt}\right)-2\left(\beta dt-1\right)u_{j+1}^{n}-\left(1-2\beta dt+\left(\omega_{0}dt\right)^{2}\right)u_{j}^{n}
\]

\end_inset


\begin_inset CommandInset line
LatexCommand rule
offset "0ex"
width "50text%"
height "0.5pt"

\end_inset


\end_layout

\begin_layout Enumerate
Backward Difference
\begin_inset Newline newline
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula 
\[
\nabla_{dx}\left[f\right]\left(x\right)=f_{j}^{n}-f_{j-1}^{n}
\]

\end_inset


\begin_inset Formula 
\[
\frac{\partial f}{\partial x}\approx\frac{\nabla_{dx}\left[f\right]\left(x\right)}{dx}=\frac{f_{j}^{n}-f_{j-1}^{n}}{dx}
\]

\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Quotes eld
\end_inset

Right
\begin_inset Quotes erd
\end_inset

 Neumann boundary:
\begin_inset Formula 
\[
f^{'}\left(t=0\right)=c\rightarrow\frac{\nabla_{dt}\left[f\right]\left(t=0\right)}{dt}=c
\]

\end_inset


\begin_inset Formula 
\[
\frac{f_{j}^{N}-f_{j}^{N-1}}{dt}=c\rightarrow f_{j}^{N-1}=f_{j}^{N}-cdt
\]

\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula 
\[
u^{''}+2\beta u^{'}+\omega_{0}^{2}u=0\rightarrow\frac{\nabla_{dt}^{2}\left[u\right]\left(t\right)}{\left(dt\right)^{2}}+2\beta\frac{\nabla_{dt}\left[u\right]\left(t\right)}{dt}+\omega_{0}^{2}u_{j}^{n}=0
\]

\end_inset


\begin_inset Formula 
\[
\frac{u_{j}^{n}-2u_{j-1}^{n}+u_{j-2}^{n}}{\left(dt\right)^{2}}+2\beta\frac{u_{j}^{n}-u_{j-1}^{n}}{dt}+\omega_{0}^{2}u_{j}^{n}=0
\]

\end_inset


\begin_inset Formula 
\[
u_{j}^{n}-2u_{j-1}^{n}+u_{j-2}^{n}+2\beta\left(u_{j}^{n}-u_{j-1}^{n}\right)dt+\omega_{0}^{2}u_{j}^{n}\left(dt\right)^{2}=0
\]

\end_inset


\begin_inset Formula 
\[
\left(1+2\beta dt+\left(\omega_{0}dt\right)^{2}\right)u_{j}^{n}-2\left(1+\beta dt\right)u_{j-1}^{n}+u_{j-2}^{n}=0
\]

\end_inset


\begin_inset Formula 
\[
u_{j+1}^{n}=\frac{2\left(1+\beta dt\right)u_{j}^{n}-u_{j-1}^{n}}{1+2\beta dt+\left(\omega_{0}dt\right)^{2}}
\]

\end_inset


\begin_inset Formula 
\[
u_{j+2}^{n}=\frac{2\left(1+\beta dt\right)u_{j+1}^{n}-u_{j}^{n}}{1+2\beta dt+\left(\omega_{0}dt\right)^{2}}
\]

\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula 
\[
u^{''}+2\beta u^{'}+\omega_{0}^{2}u=f\left(t,u,u^{'}\right)\rightarrow\frac{\nabla_{dt}^{2}\left[u\right]\left(t\right)}{\left(dt\right)^{2}}+2\beta\frac{\nabla_{dt}\left[u\right]\left(t\right)}{dt}+\omega_{0}^{2}u_{j}^{n}=f\left(t,u,\frac{\nabla_{dt}\left[u\right]\left(t\right)}{dt}\right)
\]

\end_inset


\begin_inset Formula 
\[
\frac{u_{j}^{n}-2u_{j-1}^{n}+u_{j-2}^{n}}{\left(dt\right)^{2}}+2\beta\frac{u_{j}^{n}-u_{j-1}^{n}}{dt}+\omega_{0}^{2}u_{j}^{n}=f\left(t,u_{j}^{n},\frac{u_{j}^{n}-u_{j-1}^{n}}{dt}\right)
\]

\end_inset


\begin_inset Formula 
\[
u_{j}^{n}-2u_{j-1}^{n}+u_{j-2}^{n}+2\beta\left(u_{j}^{n}-u_{j-1}^{n}\right)dt+\omega_{0}^{2}u_{j}^{n}\left(dt\right)^{2}=\left(dt\right)^{2}f\left(t,u_{j}^{n},\frac{u_{j}^{n}-u_{j-1}^{n}}{dt}\right)
\]

\end_inset


\begin_inset Formula 
\[
\left(1+2\beta dt+\left(\omega_{0}dt\right)^{2}\right)u_{j}^{n}-2\left(1+\beta dt\right)u_{j-1}^{n}+u_{j-2}^{n}=\left(dt\right)^{2}f\left(t,u_{j}^{n},\frac{u_{j}^{n}-u_{j-1}^{n}}{dt}\right)
\]

\end_inset


\begin_inset Formula 
\[
\left(1+2\beta dt+\left(\omega_{0}dt\right)^{2}\right)u_{j+2}^{n}-2\left(1+\beta dt\right)u_{j+1}^{n}+u_{j}^{n}=\left(dt\right)^{2}f\left(t,u_{j}^{n},\frac{u_{j+2}^{n}-u_{j+1}^{n}}{dt}\right)
\]

\end_inset


\begin_inset Formula 
\[
u_{j+2}^{n}=\frac{\left(dt\right)^{2}f\left(t,u_{j}^{n},\frac{u_{j+2}^{n}-u_{j+1}^{n}}{dt}\right)+2\left(1+\beta dt\right)u_{j+1}^{n}-u_{j}^{n}}{1+2\beta dt+\left(\omega_{0}dt\right)^{2}}
\]

\end_inset


\begin_inset CommandInset line
LatexCommand rule
offset "0ex"
width "50text%"
height "0.5pt"

\end_inset


\end_layout

\begin_layout Enumerate
Crank-Nicolson
\begin_inset Newline newline
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula 
\[
\delta_{dx}\left[f\right]\left(x\right)=f_{j+\frac{1}{2}}^{n}-f_{j-\frac{1}{2}}^{n}
\]

\end_inset


\begin_inset Formula 
\[
\frac{\partial f}{\partial x}\approx\frac{\delta_{dx}\left[f\right]\left(x\right)}{dx}=\frac{f_{j+\frac{1}{2}}^{n}-f_{j-\frac{1}{2}}^{n}}{dx}
\]

\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula 
\[
u^{''}+2\beta u^{'}+\omega_{0}^{2}u=0\rightarrow\frac{\delta_{dt}^{2}\left[u\right]\left(t\right)}{\left(dt\right)^{2}}+2\beta\frac{\delta_{dt}\left[u\right]\left(t\right)}{dt}+\omega_{0}^{2}u_{j}^{n}=0
\]

\end_inset


\begin_inset Formula 
\[
\frac{u_{j+1}^{n}-2u_{j}^{n}+u_{j-1}^{n}}{\left(dt\right)^{2}}+2\beta\frac{u_{j+\frac{1}{2}}^{n}-u_{j-\frac{1}{2}}^{n}}{dt}+\omega_{0}^{2}u_{j}^{n}=0
\]

\end_inset


\begin_inset Formula 
\[
u_{j+1}^{n}-2u_{j}^{n}+u_{j-1}^{n}+2\beta\left(u_{j+\frac{1}{2}}^{n}-u_{j-\frac{1}{2}}^{n}\right)dt+\omega_{0}^{2}u_{j}^{n}\left(dt\right)^{2}=0
\]

\end_inset


\begin_inset Formula 
\[
u_{j+1}^{n}-2u_{j}^{n}+u_{j-1}^{n}+2\beta\left(\frac{u_{j+1}^{n}+u_{j}^{n}}{2}-\frac{u_{j}^{n}+u_{j-1}^{n}}{2}\right)dt+\left(\omega_{0}dt\right)^{2}u_{j}^{n}=0
\]

\end_inset


\begin_inset Formula 
\[
u_{j+1}^{n}-2u_{j}^{n}+u_{j-1}^{n}+\beta\left(u_{j+1}^{n}-u_{j-1}^{n}\right)dt+\left(\omega_{0}dt\right)^{2}u_{j}^{n}=0
\]

\end_inset


\begin_inset Formula 
\[
u_{j+1}^{n}=\frac{\left(2-\left(\omega_{0}dt\right)^{2}\right)u_{j}^{n}-\left(1-\beta dt\right)u_{j-1}^{n}}{\left(1+\beta dt\right)}
\]

\end_inset


\begin_inset Formula 
\[
u_{j+2}^{n}=\frac{\left(2-\left(\omega_{0}dt\right)^{2}\right)u_{j+1}^{n}-\left(1-\beta dt\right)u_{j}^{n}}{1+\beta dt}
\]

\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula 
\[
u^{''}+2\beta u^{'}+\omega_{0}^{2}u=f\left(t,u,u^{'}\right)\rightarrow\frac{\delta_{dt}^{2}\left[u\right]\left(t\right)}{\left(dt\right)^{2}}+2\beta\frac{\delta_{dt}\left[u\right]\left(t\right)}{dt}+\omega_{0}^{2}u_{j}^{n}=f\left(t,u,\frac{\delta_{dt}\left[u\right]\left(t\right)}{dt}\right)
\]

\end_inset


\begin_inset Formula 
\[
\frac{u_{j+1}^{n}-2u_{j}^{n}+u_{j-1}^{n}}{\left(dt\right)^{2}}+2\beta\frac{u_{j+\frac{1}{2}}^{n}-u_{j-\frac{1}{2}}^{n}}{dt}+\omega_{0}^{2}u_{j}^{n}=f\left(t,u_{j}^{n},\frac{u_{j+\frac{1}{2}}^{n}-u_{j-\frac{1}{2}}^{n}}{dt}\right)
\]

\end_inset


\begin_inset Formula 
\[
u_{j+1}^{n}-2u_{j}^{n}+u_{j-1}^{n}+2\beta\left(u_{j+\frac{1}{2}}^{n}-u_{j-\frac{1}{2}}^{n}\right)dt+\omega_{0}^{2}u_{j}^{n}\left(dt\right)^{2}=\left(dt\right)^{2}f\left(t,u_{j}^{n},\frac{u_{j+\frac{1}{2}}^{n}-u_{j-\frac{1}{2}}^{n}}{dt}\right)
\]

\end_inset


\begin_inset Formula 
\[
u_{j+1}^{n}-2u_{j}^{n}+u_{j-1}^{n}+2\beta\left(\frac{u_{j+1}^{n}+u_{j}^{n}}{2}-\frac{u_{j}^{n}+u_{j-1}^{n}}{2}\right)dt+\left(\omega_{0}dt\right)^{2}u_{j}^{n}=\left(dt\right)^{2}f\left(t,u_{j}^{n},\frac{u_{j+\frac{1}{2}}^{n}-u_{j-\frac{1}{2}}^{n}}{dt}\right)
\]

\end_inset


\begin_inset Formula 
\[
u_{j+1}^{n}-2u_{j}^{n}+u_{j-1}^{n}+\beta\left(u_{j+1}^{n}-u_{j-1}^{n}\right)dt+\left(\omega_{0}dt\right)^{2}u_{j}^{n}=\left(dt\right)^{2}f\left(t,u_{j}^{n},\frac{u_{j+\frac{1}{2}}^{n}-u_{j-\frac{1}{2}}^{n}}{dt}\right)
\]

\end_inset


\begin_inset Formula 
\[
u_{j+2}^{n}=\frac{\left(dt\right)^{2}f\left(t,u_{j}^{n},\frac{u_{j+2}^{n}-u_{j}^{n}}{2dt}\right)+\left(2-\left(\omega_{0}dt\right)^{2}\right)u_{j+1}^{n}+\left(\beta dt-1\right)u_{j}^{n}}{1+\beta dt}
\]

\end_inset


